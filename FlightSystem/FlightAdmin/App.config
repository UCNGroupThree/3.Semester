<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAdministratorService" />
                <binding name="BasicHttpBinding_IAirportService" />
                <binding name="BasicHttpBinding_IFlightService" />
                <binding name="BasicHttpBinding_IPlaneService" />
                <binding name="BasicHttpBinding_IRouteService" />
                <binding name="BasicHttpBinding_IUserService" />
                <binding name="BasicHttpBinding_IDijkstra" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IReservationService">
                    <reliableSession enabled="true" />
                    <security mode="None" />
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://azureflightwcf.cloudapp.net/AzureService.svc/Administrator"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAdministratorService"
                contract="MainService.IAdministratorService" name="BasicHttpBinding_IAdministratorService" />
            <endpoint address="http://azureflightwcf.cloudapp.net/AzureService.svc/Airport"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAirportService"
                contract="MainService.IAirportService" name="BasicHttpBinding_IAirportService" />
            <endpoint address="http://azureflightwcf.cloudapp.net/AzureService.svc/Flight"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IFlightService"
                contract="MainService.IFlightService" name="BasicHttpBinding_IFlightService" />
            <endpoint address="http://azureflightwcf.cloudapp.net/AzureService.svc/Plane"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPlaneService"
                contract="MainService.IPlaneService" name="BasicHttpBinding_IPlaneService" />
            <endpoint address="http://azureflightwcf.cloudapp.net/AzureService.svc/Route"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRouteService"
                contract="MainService.IRouteService" name="BasicHttpBinding_IRouteService" />
            <endpoint address="http://azureflightwcf.cloudapp.net/AzureService.svc/User"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUserService"
                contract="MainService.IUserService" name="BasicHttpBinding_IUserService" />
            <endpoint address="http://azureflightwcf.cloudapp.net/AzureService.svc/Dijkstra"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IDijkstra"
                contract="MainService.IDijkstra" name="BasicHttpBinding_IDijkstra" />
            <endpoint address="http://azureflightwcf.cloudapp.net/AzureService.svc/Reservation"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IReservationService"
                contract="MainService.IReservationService" name="WSHttpBinding_IReservationService" />
        </client>
    </system.serviceModel>
</configuration>